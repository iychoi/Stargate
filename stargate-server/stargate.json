{
  "daemon_driver_setting" : [ {
    "driver_class" : "stargate.drivers.hazelcast.HazelcastCoreDriver",
    "driver_configuration" : {
      "@class" : "stargate.drivers.hazelcast.HazelcastCoreDriverConfiguration",
      "port" : 21010,
      "known_host" : [ ],
      "my_host" : "localhost"
    }
  } ],
  "name" : "StargateTestService",
  "source_filesystem" : {
    "driver_setting" : {
      "driver_class" : "stargate.drivers.sourcefs.hdfs.HDFSSourceFileSystemDriver",
      "driver_configuration" : {
        "@class" : "stargate.drivers.sourcefs.hdfs.HDFSSourceFileSystemDriverConfiguration",
        "root_path" : "/"
      }
    }
  },
  "recipe_generator" : {
    "driver_setting" : {
      "driver_class" : "stargate.drivers.recipe.sha1fixed.SHA1FixedChunkRecipeGeneratorDriver",
      "driver_configuration" : {
        "@class" : "stargate.drivers.recipe.sha1fixed.SHA1FixedChunkRecipeGeneratorDriverConfiguration",
        "chunk_size" : 1048576
      }
    }
  },
  "schedule" : {
    "driver_setting" : {
      "driver_class" : "stargate.drivers.hazelcast.schedule.HazelcastScheduleDriver",
      "driver_configuration" : {
        "@class" : "stargate.drivers.hazelcast.schedule.HazelcastScheduleDriverConfiguration",
        "task_threads" : 4
      }
    }
  },
  "transport" : {
    "driver_setting" : {
      "driver_class" : "stargate.drivers.transport.http.HTTPTransportDriver",
      "driver_configuration" : {
        "@class" : "stargate.drivers.transport.http.HTTPTransportDriverConfiguration",
        "service_host_name_pattern" : ".*",
        "service_port" : 31010,
        "thread_pool_size" : 10
      }
    }
  },
  "user_interface" : {
    "driver_setting" : [ {
      "driver_class" : "stargate.drivers.userinterface.http.HTTPUserInterfaceDriver",
      "driver_configuration" : {
        "@class" : "stargate.drivers.userinterface.http.HTTPUserInterfaceDriverConfiguration",
        "service_port" : 41010
      }
    } ]
  },
  "datastore" : {
    "driver_setting" : {
      "driver_class" : "stargate.drivers.hazelcast.datastore.HazelcastDataStoreDriver",
      "driver_configuration" : {
        "@class" : "stargate.drivers.hazelcast.datastore.HazelcastDataStoreDriverConfiguration"
      }
    }
  }
}
